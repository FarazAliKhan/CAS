@page
@model WebRazor.Pages.DetailsModel
@{
}

<div class="text-center"><h1>View CAS</h1></div>
<section class="center">
    <div class="form-group">
        <div class="form-control row center">
            <label for="firstname">First Name: </label>
            <label id="firstname">@Model.firstName</label>
        </div>
    </div>
    <div class="form-group">
        <div class="form-control row center">
            <label for="lastname">Last Name: </label>
            <label id="lastname">@Model.lastName</label>
        </div>
    </div>
    <div class="form-group">
        <div class="form-control row center">
            <label for="dateofbirth">Date of Birth: </label>
            <label id="dateofbirth">@Model.dob.ToString()</label>
        </div>
    </div>
</section>
<div>
    <a class="btn btn-default" asp-page="./Index">Back</a>
</div>